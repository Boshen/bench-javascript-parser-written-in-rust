{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": true,
  "packageRules": [
    {
      "matchPackagePatterns": ["oxc"],
      "groupName": "oxc",
      "automerge": true
    },
    {
      "matchPackagePatterns": ["swc"],
      "groupName": "swc",
      "automerge": true
    },
    {
      "matchPackagePatterns": ["rome"],
      "groupName": "rome",
      "automerge": true
    }
  ]
}
